#include<iostream>
#include<vector>
#include<algorithm>
#include<iomanip>
#include<utility>
#include<iomanip>
#include<map>
#include<cmath>
#include<cstdio>
#define rep(i,n) for(int i = 0; i < (n); ++i)
using namespace std;
using ll =long long;
using P = pair<int,int>;

int main(){

    double n,y,l=0,m=0;
    cin >> n >> y;
    double a=n;
    n=0;
    vector<double> M(3,0);
    M[0]=10000; M[1]=5000; M[2]=1000;

    for(;l<a;l++){
        for(;m<a-l;m++){
            for(;n<a-l-m;n++){
                if(M[0]*l + M[1]*m + M[2]*n == y){
                    cout << l << " " << m << " " << n << endl; 
                    return 0;
                }
            }
        }
    }
    cout << "-1 -1 -1" << endl;
    return 0;
}
